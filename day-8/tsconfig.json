{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES2019", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
